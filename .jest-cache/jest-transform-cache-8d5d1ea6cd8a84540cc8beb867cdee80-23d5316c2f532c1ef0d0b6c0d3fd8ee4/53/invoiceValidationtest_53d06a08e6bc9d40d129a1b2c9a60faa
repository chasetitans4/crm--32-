const _invoiceValidation = require("../../utils/invoiceValidation");
describe("invoiceValidation", ()=>{
    describe("validateEmail", ()=>{
        it("validates correct email addresses", ()=>{
            expect((0, _invoiceValidation.validateEmail)("test@example.com")).toBe(true);
            expect((0, _invoiceValidation.validateEmail)("user.name@domain.co.uk")).toBe(true);
        });
        it("rejects invalid email addresses", ()=>{
            expect((0, _invoiceValidation.validateEmail)("invalid-email")).toBe(false);
            expect((0, _invoiceValidation.validateEmail)("test@")).toBe(false);
            expect((0, _invoiceValidation.validateEmail)("@domain.com")).toBe(false);
        });
    });
    describe("validatePhone", ()=>{
        it("validates correct phone numbers", ()=>{
            expect((0, _invoiceValidation.validatePhone)("1234567890")).toBe(true);
            expect((0, _invoiceValidation.validatePhone)("+1 (555) 123-4567")).toBe(true);
        });
        it("rejects invalid phone numbers", ()=>{
            expect((0, _invoiceValidation.validatePhone)("abc")).toBe(false);
            expect((0, _invoiceValidation.validatePhone)("123")).toBe(false);
        });
    });
    describe("validateAmount", ()=>{
        it("validates correct amounts", ()=>{
            expect((0, _invoiceValidation.validateAmount)("100")).toBe(true);
            expect((0, _invoiceValidation.validateAmount)("100.50")).toBe(true);
            expect((0, _invoiceValidation.validateAmount)("0")).toBe(true);
        });
        it("rejects invalid amounts", ()=>{
            expect((0, _invoiceValidation.validateAmount)("-100")).toBe(false);
            expect((0, _invoiceValidation.validateAmount)("abc")).toBe(false);
        });
    });
    describe("validateContractForm", ()=>{
        const validFormData = {
            clientName: "John Doe",
            clientEmail: "john@example.com",
            clientPhone: "1234567890",
            projectTitle: "Website Redesign",
            projectDescription: "Complete website redesign",
            totalAmount: "5000",
            depositPercentage: "50",
            startDate: "2024-01-01",
            endDate: "2024-01-31",
            deliveryDate: "2024-02-01",
            paymentTerms: "Net 30",
            deliverables: [
                "Design",
                "Development"
            ]
        };
        it("validates correct form data", ()=>{
            const result = (0, _invoiceValidation.validateContractForm)(validFormData);
            expect(result.isValid).toBe(true);
            expect(result.errors).toHaveLength(0);
        });
        it("rejects form with missing required fields", ()=>{
            const invalidFormData = {
                ...validFormData,
                clientName: ""
            };
            const result = (0, _invoiceValidation.validateContractForm)(invalidFormData);
            expect(result.isValid).toBe(false);
            expect(result.errors.some((error)=>error.field === "clientName")).toBe(true);
        });
        it("rejects form with invalid email", ()=>{
            const invalidFormData = {
                ...validFormData,
                clientEmail: "invalid-email"
            };
            const result = (0, _invoiceValidation.validateContractForm)(invalidFormData);
            expect(result.isValid).toBe(false);
            expect(result.errors.some((error)=>error.field === "clientEmail")).toBe(true);
        });
    });
    describe("validateInvoiceForm", ()=>{
        const validFormData = {
            invoiceNumber: "INV-001",
            clientName: "John Doe",
            clientEmail: "john@example.com",
            clientAddress: "123 Main St",
            issueDate: "2024-01-01",
            dueDate: "2024-02-01",
            items: [],
            subtotal: 0,
            tax: 0,
            totalAmount: 0,
            notes: "Thank you",
            status: "Draft"
        };
        const validItems = [
            {
                id: "1",
                description: "Web Design",
                quantity: 1,
                price: 1000,
                total: 1000
            }
        ];
        it("validates correct form data", ()=>{
            const result = (0, _invoiceValidation.validateInvoiceForm)(validFormData, validItems);
            expect(result.isValid).toBe(true);
            expect(result.errors).toHaveLength(0);
        });
        it("rejects form with no items", ()=>{
            const result = (0, _invoiceValidation.validateInvoiceForm)(validFormData, []);
            expect(result.isValid).toBe(false);
            expect(result.errors.some((error)=>error.field === "items")).toBe(true);
        });
        it("rejects form with invalid due date", ()=>{
            const invalidFormData = {
                ...validFormData,
                dueDate: "2023-12-01"
            };
            const result = (0, _invoiceValidation.validateInvoiceForm)(invalidFormData, validItems);
            expect(result.isValid).toBe(false);
            expect(result.errors.some((error)=>error.field === "dueDate")).toBe(true);
        });
    });
});

