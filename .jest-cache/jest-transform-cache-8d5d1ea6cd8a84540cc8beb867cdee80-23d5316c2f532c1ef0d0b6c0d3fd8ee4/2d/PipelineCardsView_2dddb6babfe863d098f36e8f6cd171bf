            /* istanbul ignore next */
            (cov_oj3ezwew6().b[39][0]++, hoveredStage === stage.id) &&
            /* istanbul ignore next */
            (cov_oj3ezwew6().b[39][1]++, /*#__PURE__*/(0, _jsxruntime.jsx)("div", {
              className: "text-xs text-primary-600",
              children: "Drop here to move client"
            }))]
          })),
          /* istanbul ignore next */
          (cov_oj3ezwew6().b[40][0]++, viewMode === 'kanban') &&
          /* istanbul ignore next */
          (cov_oj3ezwew6().b[40][1]++, /*#__PURE__*/(0, _jsxruntime.jsxs)("div", {
            className: "pt-3 border-t flex justify-between items-center",
            children: [/*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
              className: "text-xs text-gray-500 hover:text-gray-700 flex items-center",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Plus, {
                className: "h-3 w-3 mr-1"
              }), "Add client"]
            }), /*#__PURE__*/(0, _jsxruntime.jsxs)("button", {
              className: "text-xs text-gray-500 hover:text-gray-700 flex items-center",
              children: [/*#__PURE__*/(0, _jsxruntime.jsx)(_lucidereact.Zap, {
                className: "h-3 w-3 mr-1"
              }), "Automate"]
            })]
          }))]
        })
      }, stage.id);
    })
  });
};
const _default =
